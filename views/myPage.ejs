<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge" />
    <link rel="stylesheet" href="/stylesheets/style.css" />
    <title>My Page</title>
  </head>
  <body>
    <h1>My page</h1>
    <a href="/votings/new" class="create-vote">+투표</a>
    <% myVotes.forEach(vote => { %>
    <a href="/votings/<%= vote._id %>" class="vote-element-<%= vote.inProgress %>">
      <div class="vote-title"><%= vote.title %></div>
      <div class="vote-expiration">만료: <%= vote.expiration%></div>
      <div class="vote-created_at">생성일: <%= vote.created_at %></div>
    </a>
    <% }); %>
  </body>
  <script>
    const expiredVote = document.querySelectorAll('.vote-element-false');
    expiredVote.forEach(vote => {
      vote.removeAttribute('href');
    });
  </script>
</html>
